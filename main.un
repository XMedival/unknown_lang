import "std.un"
import "io.un"

mem file 1024

fn main(argc, argv)
    if argc > 1
        print "Opening: "
        print @argv[1]
        print "\n"
        fd = open @argv[1], 0
        read fd, file, 1024
        close fd
        print file
        ret 0
    else
        eprint "Please provide an input file\n"
        ret 1
    end
end
